{
  "private": true,
  "name": "@xen-orchestra/proxy",
  "version": "0.0.0",
  "license": "ISC",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/vatesfr/xen-orchestra/tree/master/@xen-orchestra/proxy",
  "bugs": "https://github.com/vatesfr/xen-orchestra/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatesfr/xen-orchestra.git"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {
    "xo-proxy": "dist/main"
  },
  "files": [
    "dist/"
  ],
  "browserslist": [
    ">2%"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@xen-orchestra/async-map": "^0.0.0",
    "@xen-orchestra/defined": "^0.0.0",
    "@xen-orchestra/emit-async": "^0.0.0",
    "@xen-orchestra/fs": "^0.0.0",
    "@xen-orchestra/log": "^0.0.0",
    "app-conf": "^0.5.0",
    "http-server-plus": "^0.10.0",
    "jest-worker": "^22.4.3",
    "json-rpc-protocol": "^0.11.4",
    "jsonwebtoken": "^8.2.1",
    "koa": "^2.5.1",
    "lodash": "^4.17.10",
    "promise-toolbox": "^0.9.5",
    "stoppable": "^1.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-flow": "^7.0.0-beta.47",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-lodash": "^3.3.2",
    "cross-env": "^5.1.5",
    "index-modules": "^0.3.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "jest --bail --watch",
    "posttest": "jest",
    "prebuild": "npm run clean && index-modules --cjs-lazy src/main/app/mixins",
    "predev": "npm run prebuild",
    "prepublishOnly": "npm run build",
    "pretest": "eslint --ignore-path .gitignore --fix .",
    "test": "flow status"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "\\.spec\\.js$"
  }
}
